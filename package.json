{
  "name": "tsas",
  "version": "0.0.17",
  "description": "A command line tool for AWS Lambda TypeScript function.",
  "preferGlobal": true,
  "bin": {
    "tsas": "bin/tsas.js"
  },
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "watch": "webpack --config webpack.config.js --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "APP_NAME": "test-app",
      "VERSION": "0.0.1",
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/*.+(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cm-wada-yusuke/tsas.git"
  },
  "keywords": [
    "TypeScript",
    "AWS",
    "Lambda"
  ],
  "author": "cm-wada.yusuke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cm-wada-yusuke/tsas/issues"
  },
  "homepage": "https://github.com/cm-wada-yusuke/tsas#readme",
  "devDependencies": {
    "@types/archiver": "^2.1.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^24.0.13",
    "@types/js-yaml": "^3.12.0",
    "@types/node": "^10.12.18",
    "@types/readline-sync": "^1.4.3",
    "@types/uuid": "^3.4.4",
    "@types/yargs": "^12.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.5",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "aws-sdk": "^2.393.0",
    "colors": "^1.3.3",
    "fs-extra": "^7.0.1",
    "js-yaml": "^3.13.1",
    "proxy-agent": "^3.0.3",
    "readline-sync": "^1.4.9",
    "source-map-support": "^0.5.10",
    "uuid": "^3.3.2",
    "yargs": "^12.0.5"
  }
}
